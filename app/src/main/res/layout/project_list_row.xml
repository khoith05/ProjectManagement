<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/projectLayout"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="10dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:background="@color/teal_700">

    <TextView
        android:id="@+id/name_project"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dadsd"
        android:textColor="@color/black" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/description_project"
        android:text="dadsdaaaaaaaaaaaaa"
        android:textColor="@color/black"
        android:layout_below="@id/name_project"
        />

    <TextView
        android:id="@+id/date_end_project"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:text="20/11/2021" />

    <ImageView
        android:id="@+id/kebabProject"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:clickable="true"
        android:focusable="auto"
        android:contentDescription="backEmployee"
        android:layout_alignParentRight="true"
        app:srcCompat="@drawable/kebab"/>
</RelativeLayout>